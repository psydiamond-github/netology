
[bastionhost]
bastion-host ansible_host=51.250.69.56 ansible_ssh_user=slynko

[webservers]
web1 ansible_host=192.168.10.23
web2 ansible_host=192.168.20.21

[prometheus]
prometheus-host ansible_host=192.168.30.11

[grafana]
grafana-host ansible_host=192.168.30.6

[elasticsearch]
